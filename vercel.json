{
  "version": 2,
  "builds": [{ "src": "src/main.ts", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
    },
    {
      "src": "/notifications",
      "dest": "src/main.ts",
      "methods": ["GET","POST","OPTIONS"]
    }
  ]
}
